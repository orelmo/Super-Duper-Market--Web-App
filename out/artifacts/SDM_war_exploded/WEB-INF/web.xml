        <servlet-name>FeedbackServlet</servlet-name>
        <url-pattern>/Pages/FeedbackPage/getFeedbackableStores</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeedbackServlet</servlet-name>
        <url-pattern>/Pages/FeedbackPage/submitFeedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OrderHistoryServlet</servlet-name>
        <url-pattern>/Pages/OrderHistoryPage/getOrderHistory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StoreOrderHistoryServlet</servlet-name>
        <url-pattern>/Pages/StoreOrderHistoryPage/getStoreOrderHistory</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SellerZoneFeedbacksServlet</servlet-name>
        <url-pattern>/Pages/SellerZoneFeedbacksPage/getSellerZoneFeedbacks</url-pattern>
    </servlet-mapping>
