    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Pages/LoginPage/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OnlineUsersServlet</servlet-name>
        <url-pattern>/Pages/ZonesPage/onlineUsers</url-pattern>
    </servlet-mapping>
