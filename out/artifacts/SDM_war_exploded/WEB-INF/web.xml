         version="4.0">

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OnlineUsersServlet</servlet-name>
        <servlet-class>Servlets.OnlineUsersServlet</servlet-class>
